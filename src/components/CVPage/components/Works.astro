 }

 .work_table {
     margin-top: 0.3em;
 }

 .work_item {
     margin-top: 0.4em;
 }
 
 ul.work_table > .work_item {
     margin-left: -1em; !important;
 }

 ol.work_table > .work_item {
     list-style-type: decimal !important;

 }

 ol.work_table > li:before {
     width: 0px !important;
 }

 .awarded_work_tag {
